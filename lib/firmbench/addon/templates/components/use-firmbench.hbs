<div class="slide-modal firm-modal firm-effect-7{{if firmBenchModal ' firm-show' ' firm-hide'}}">
  <div class="firm-content">
    <div class="container">
      <div class="row">
        <div class="box-1x1">
          <form action="#">
            <a href="#" id="hide" class="close-btn" {{action 'showFirmBenchModal'
            false}}><span>X</span></a>
            <div class="clearfix"></div>
            <h2>Compare your firm</h2>
            <ul class="form-compare">
              <li>{{input type="text" value=firmName id="firmName" size="60" min="1" max="100000"
                placeholder="Firm name"}} {{#validation-error-field submitted=submitted
                className="error" tagName="label" for="firmName" field="firmName" validation=nameValidation}}Please
                enter valid Firm name{{/validation-error-field}}</li>
              <li>{{input type="text" value=email id="email" size="60" min="1" max="100000"
                placeholder="Email address"}} {{#validation-error-field submitted=submitted
                className="error" field="email" tagName="label" for="email" validation=emailValidation}}Please
                enter valid Email address{{/validation-error-field}}</li>
              <!-- <li class="side-by-side"><span class="symbol-key">#</span>{{#x-select value=firmSizevalue}} {{#x-option value=''}}Employees{{/x-option}}
              {{#each-in firmSize as |key data|}} {{#if data.size.key}} {{#x-option value=data.size.key}}{{data.size.key}}{{/x-option}}
              {{/if}} {{/each-in}} {{/x-select}}</li> -->
              <li class="side-by-side">
                <span class="symbol-key">#</span>{{input type="text" value=firmSizevalue id="firmSize" size="60"
                min="1" max="100000" placeholder="Employees"}} {{#validation-error-field
                submitted=submitted class="error" field="even" tagName="label" for="firmSize"
                validation=firmSizeValidation}}Please enter valid Firmsize(ex:1 to 100000){{/validation-error-field}}</li>
              <li
              class="side-by-side no-margin">{{input type="text" value=firmBenchProfit id="profit" size="60" placeholder="Profit as a % of net billings"}}
              
                <span class="symbol-key">%</span>{{#validation-error-field submitted=submitted class="error" field="even"
                tagName="label" for="profit" validation=firmBenchProfitValidation}}Please
                enter valid Profitability Value(ex:-100 to 100){{/validation-error-field}}</li>
                <li
                class="side-by-side">
                  <span class="symbol-key">$</span>{{input type="text" value=firmBenchNet id="netRevenue" size="60"
                  placeholder="Annual net revenue"}} {{#validation-error-field submitted=submitted
                  class="error" field="even" tagName="label" for="netRevenue" validation=firmBenchNetValidation}}Please
                  enter valid Netrevenue Value(ex:-999999999 to 999999999){{/validation-error-field}}</li>
                  <li
                  class="side-by-side no-margin">{{input type="text" value=firmBenchRepeat id="repeatClients" size="60"
                    placeholder="Share of billings from repeat clients"}}
                    <span class="symbol-key">%</span>{{#validation-error-field submitted=submitted class="error" field="even"
                    tagName="label" for="repeatClients" validation=firmBenchRepeatValidation}}Please
                    enter valid Repeatclient Value(ex:0 to 100){{/validation-error-field}}</li>
                    <!--
                    last-one -->
                    <li class="side-by-side select-box">{{#x-select value=firmSpecializationValue}} {{#x-option value=''}}Firm
                      specialization{{/x-option}} {{#each-in firmSpecialization as |key data|}}
                      {{#if data.specialization.key}} {{#x-option value=data.specialization.key}}{{data.specialization.key}}{{/x-option}}
                      {{/if}} {{/each-in}} {{/x-select}}</li>
                    <li class="side-by-side button-box no-margin">
                      <input type="button" value="Compare" class="common-btn" {{action
                      'submitfirmBench'}}>
                    </li>
                    <li>
                      <!-- <a href="#" class="common-btn">Submit</a> -->
                      <!-- <button type="submit">Submit</button> -->
                    </li>
            </ul>
          </form>
          <div class="disclaimer-firm">
